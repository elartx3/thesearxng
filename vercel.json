{
  "functions": {
    "php/intellect.php": {
      "runtime": "vercel-php@0.7.1",
      "maxDuration": 60
    },
    "php/search.php": {
      "runtime": "vercel-php@0.7.1",
      "maxDuration": 60
    },
    "php/autocompleter.php": {
      "runtime": "vercel-php@0.7.1",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/favicon/(.*)",
      "dest": "/favicon/$1"
    },
    {
      "src": "/search",
      "dest": "php/search.php"
    },
    {
      "src": "/autocompleter",
      "dest": "php/autocompleter.php"
    },
    {
      "src": "/(.*)",
      "dest": "php/intellect.php"
    }
  ]
}
